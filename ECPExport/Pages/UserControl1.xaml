<UserControl x:Class="ECPExport.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             x:Name="ctl" Width="686" Height="410">

    <Grid Margin="0,-150,0,0" HorizontalAlignment="Left" Width="700" Height="400">
        <TabControl IsSynchronizedWithCurrentItem="True" Background="#FF3E3E46" Height="370">
            <TabItem Header="ECP - Export" Background="Black" Margin="0,0,0,0" >
                <Button x:Name="Export" Content="Exportar Entidades" Margin="88,194,128,37" Click="Export_Click"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
